{
  "cli": {
    "version": ">= 14.1.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "node": "22.11.0"
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "distribution": "store"
      },
      "env": {
        "SENTRY_DISABLE_AUTO_UPLOAD": "true",
        "EAS_NO_VCS": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "SENTRY_DISABLE_AUTO_UPLOAD": "true"
      }
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "appleId": "fallenearly@gmail.com",
        "ascAppId": "6756468628"
      }
    },
    "production": {}
  }
}